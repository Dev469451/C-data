#include <iostream>
using namespace std;

class Student {
public:
    int RollNo;
    string name;
    // bool scholarship; // fixed spelling

    Student(){
        cout<< "Constructor ran"<<endl;
    }
    void getname(){
        cout<< "Enter name: " << name << endl;
    }

};

int main() {
    Student Sahil;
    Sahil.RollNo = 101;
    Sahil.name = "Sahil";
   

    Student Dev;
    Dev.RollNo = 102;
    Dev.name = "Dev";

    Sahil.getname();
    Dev.getname();
    Student John;
    John.getname(); 

    return 0;
}
