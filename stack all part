#include <bits/stdc++.h>
using namespace std;

class Stack {
public:
    int arr[10];
    int top;

    Stack() {
        top = -1; 
    }

    void push(int value) {
        if (top == 9) {  
            cout << "Stack is Full / Overflow" << endl;
        } else {
            top++;
            arr[top] = value;
        }
    }

    void pop() { 
        if (top == -1) {
            cout << "Stack is Empty / Underflow" << endl;
        } else {
            top--;
        }
    }

    bool isEmpty() {
        if (top == -1) {
            return true; 
        } else {
            return false;
        }
    }

    int peek() {
        if (top == -1) {
            cout << "Stack is Empty!" << endl;
            return -1; 
        }
        return arr[top];
    }

    void traversal() {
        if (top == -1) {
            cout << "Stack is Empty!" << endl;
            return;
        }

        for (int i = top; i >= 0; i--) {
            cout << arr[i] << " ";
        }
        cout << endl;
    }
};

int main() {
    Stack st;

    st.push(100);
    st.push(150);
    st.push(160);
    st.push(20);

    st.traversal();

    st.pop();
    st.traversal();


    return 0;
}
