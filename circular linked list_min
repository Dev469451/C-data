#include <iostream>
using namespace std;

struct Node {
    int data;
    Node* next;

    Node(int a) {
        data = a;
        next = NULL;
    }
};

void printLLCircular(Node* head) {
    if (!head) return;
    Node* p = head;
    do {
        cout << p->data << " ";
        p = p->next;
    } while (p != head);
}

int minCircular(Node* head) {
    // if (!head) return 1e9;
    int minVal = head->data;
    Node* p = head->next;

    while (p != head) {
        if (p->data < minVal)
            minVal = p->data;
        p = p->next;
    }
    return minVal;
}

int main() {
   
    Node* head = new Node(12);
    head->next = new Node(22);
    head->next->next = new Node(32);
    head->next->next->next = new Node(42);
    head->next->next->next->next = new Node(52);
    
    head->next->next->next->next->next = head;  

    printLLCircular(head);
    cout << endl;

    int total = minCircular(head);
    cout << "Min value: " << total << endl;

    return 0;
}

    
