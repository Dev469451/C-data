#include <bits/stdc++.h>
using namespace std;

int main() {
    int arr[5] = {1, 3, 5, 4, 1};

    for (int i = 0; i < 5; i++) {
        if (i % 2 == 0) {
            cout << arr[i] << " ";
        }
    }

    return 0;
}


