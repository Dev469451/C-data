#include <iostream>
using namespace std;

struct Node {
    int data;
    Node* next;

    Node(int a) {
        data = a;
        next = NULL;
    }
};

void printLLCircular(Node* head) {
    if (!head) return;
    Node* p = head;
    do {
        cout << p->data << " ";
        p = p->next;
    } while (p != head);
}

void deletionCircularHead(Node*& head) {
    if (!head) return;

    if (head->next == head) {
        delete head;
        head = nullptr;
        return;
    }

    Node* p = head;
    while (p->next != head) {
        p = p->next;
    }

    Node* temp = head;
    p->next = head->next;
    head = head->next;
    delete temp;
}

int main() {
    Node* head = new Node(12);
    head->next = new Node(22);
    head->next->next = new Node(32);
    head->next->next->next = new Node(42);
    head->next->next->next->next = new Node(52);
    
    head->next->next->next->next->next = head;
    
    printLLCircular(head);
    cout << endl;
    deletionCircularHead(head);
    
    printLLCircular(head);

    return 0;
}
