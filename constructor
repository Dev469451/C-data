#include <bits/stdc++.h>
using namespace std;

class student {
public:
    int age;
    string name;

    // Constructor with default parameters
    student(int ageOfstudent = 20, string nameOfstudent = "Unknown") {
        age = ageOfstudent;
        name = nameOfstudent;
    }
};

int main() {
    student s1(23, "Nikhil");
    student s3(23);  // name will take default value "Unknown"

    cout << s1.age << " " << s1.name << endl;
    cout << s3.age << " " << s3.name << endl;

    return 0;
}
