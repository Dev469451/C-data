#include <iostream>
using namespace std;
 class Student{
     public:
     int age, rollno;
     
     void setValue(int a, int b){
         age = a;
         rollno = b;
     }
     void setValue(){
         cout <<age << endl;
         cout<< rollno <<endl;
     }
     
 };
 
  int main(){
      
      Student s1;
      s1.age = 23;
      s1.setValue(25,100);
      s1.setValue();
      
      return 0;
  }
