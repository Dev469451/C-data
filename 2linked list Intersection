#include<iostream>
using namespace std;

   struct Node{
       int data;
       Node* next;
       
       Node(int a){
           data = a;
           next = NULL;
       }
   };
   
  void printLL(Node* head){
      Node* p = head;
      
      while(p!= NULL){
          cout << p-> data << " ";
          p = p->next;
      }
  }
   
  
int main(){
    
     Node* head = new Node(12);
     head ->next = new Node(22);
     head ->next->next = new Node(32);
     head ->next->next->next = new Node(42);
     head ->next->next->next->next = new Node(52);
    
    Node* head2 = new Node(34);
    head2 ->next = new Node(44);
    head2 ->next->next = new Node(32);
    head2 ->next->next->next = head->next->next;
    
    Node* p = head;
    Node* q = head2;
    
    while(p != q){
        if(p == nullptr){
            p = head2;
        }else
        p = p->next;
        
        if(q == nullptr){
            q = head;
        }else 
        q = q->next;
    }
    
    cout << "Intersection: " << p->data;
    
    return 0;
}

